\t\t.then((res) => {\r\n\t\t\tconsole.log('(Frontend getBooksFromDb) Fetched items: ');\r\n\t\t\tconsole.log(res);\r\n\t\t\tthis.books = res;\r\n\t\t\tconsole.log('(Frontend getBooksFromDb) The current items in the instance are: ');\r\n\t\t\tconsole.log(this.books);\r\n\t\t\t});\r\n\t\t}\r\n\t},\r\n\tcreated: function(){\r\n\t\tthis.getBooksFromDb();\r\n\t}\r\n}\r\n</script>\r\n\r\n<style>\r\n#item-title{\r\n\tfont-weight: bold;\r\n}\r\n#item-subtitle{\r\n\tfont-size:13px;\r\n\tfont-style: italic;\r\n}\r\n</style>"],"sourceRoot":""}]}